tabla usuarios
	-idUsuario(pk)
	-idPermisos(fk)
	-email
	-password
tabla permisos
	-idPermisos(pk)
	-nombre
	-descripcion
tabla encuestas
	-idEncuestas(pk)
	-idUsuario(fk)
	-nombre
	-multirespuesta(booleano)
	-descripcion(opcional)
tabla opcionesEncuestas
	-idOpciones(pk)
	-idEncuestas(fk)
	-nombre
tabla votosEncuestas
	-idVotos(pk)
	-idOpciones
	-idUsuario
tabla accesoEncuestas
	-idAcceso(pk)
	-idUsuario
	-idEncuestas

